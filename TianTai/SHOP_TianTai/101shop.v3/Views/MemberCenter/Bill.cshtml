@using System.Data;
@using System.Collections.Specialized;
@using SOSOshop.Model;
@{
    ViewBag.Title = (string.IsNullOrEmpty(ViewBag.TrueName) ? ViewBag.MobilePhone : ViewBag.TrueName) + "的会员中心";
    Layout = "~/Views/Shared/_Master.cshtml";
}
@section head
{
    <script type="text/javascript" src="/Scripts/tab.js"></script>
    <script type="text/javascript" src="/Scripts/jquery-1.5.1.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $(".topC0").click(function () {
                $(".topC0").removeClass("topC1");
                $(this).addClass("topC1");
                $(".NewsTop_cnt .d_01").hide();
                $(".NewsTop_cnt .d_01").eq($(this).index()).show();
            });
        });
    </script>
}
@{Html.RenderPartial("Nav");}
<ul class="list_nav">
    <li><a href="/MemberCenter/">会员中心</a> &gt; <span>我的往来帐</span></li>
</ul>
@{Html.RenderPartial("MemberCenter_LeftMenu");}
<div class="list_right_detail" style="width: 800px; float: left;">
    <div class="info_Retrieval7 irf">
        <h3>
            账户金额</h3>
            <br />
            <p>您当前的可用金额为：<strong>￥0.00元</strong>
            近一年实际消费额：<strong>￥0.00元</strong>
            <span><a href="/MemberCenter/">查看你的会员级别</a></span></p>
    </div>
    <div class="info_Retrieval5 irf">
        <table cellpadding="0" cellspacing="0" border="0" style="width: 800px; height: 40px;">
            <tr>
                <td>
                    <span style="height: 22px; padding-top: 5px; font-weight: bold;">@ViewBag.IncName</span>
                    <span style="display: block; height: 22px; padding-top: 5px; font-weight: bold; color:Red;">导出Excel</span>
                </td>
            </tr>
        </table>
        <table cellpadding="0" cellspacing="0" border="0" style="width: 800px; height: 40px;">
            <tr>
                <th>日期</th>
                <th>单据编号</th>
                <th>摘要</th>
                <th>销售额</th>
                <th>收入金额</th>
                <th>应收余额</th>
            </tr>
            <tr>
                <td></td>
